{
  "bindings": [
    {
      "authLevel": "function",
      "name": "req",
      "type": "httpTrigger",
      "direction": "in",
      "route": "api/v1/bonus/vacanze/activations/{fiscalcode}",
      "methods": ["post"]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "name": "starter",
      "type": "durableClient",
      "direction": "in"
    },
    {
      "type": "queue",
      "direction": "out",
      "name": "bonusActivation",
      "queueName": "bonusactivations",
      "connection": "BONUS_STORAGE_CONNECTION_STRING"
    }
  ],
  "scriptFile": "../dist/StartBonusActivation/index.js"
}
