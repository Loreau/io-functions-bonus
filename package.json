{
  "name": "io-functions-bonus",
  "description": "",
  "author": "IO team",
  "repository": "https://github.com/pagopa/io-functions-bonus",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "prestart": "func extensions install --javascript",
    "start": "func start --javascript",
    "predev": "npm-run-all generate build",
    "dev": "npm-run-all --parallel start watch",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "tslint -p .",
    "lint-api": "oval validate -p openapi/index.yaml",
    "dist:modules": "modclean -r -n default:safe && yarn install --production",
    "dist:zip": "zip -r dist.zip . --exclude @.funcignore --exclude .funcignore",
    "predeploy": "npm-run-all generate build dist:modules",
    "generate:definitions": "shx rm -rf ./generated/definitions && shx mkdir -p ./generated/definitions && gen-api-models --api-spec ./openapi/index.yaml --out-dir ./generated/definitions",
    "generate": "npm-run-all generate:*",
    "generate:ts-from-yaml:SvcConsultazione": "shx mkdir -p generated/SvcConsultazione && rimraf generated/SvcConsultazione/* && gen-api-models --strict 0 --api-spec generated/yaml-file/SvcConsultazione.yaml --out-dir generated/SvcConsultazione",
    "generate:yaml-from-wsdl": "shx mkdir -p generated/yaml-file && rimraf generated/yaml-file/* && node generator/yamlGenerator.js"
  },
  "devDependencies": {
    "@azure/functions": "^1.0.3",
    "@types/bluebird": "^3.5.32",
    "@types/documentdb": "^1.10.5",
    "@types/express": "^4.16.0",
    "@types/jest": "^24.0.15",
    "@types/soap": "^0.21.0",
    "@types/vfile-message": "^2.0.0",
    "apiconnect-wsdl": "^1.8.21",
    "danger": "^8.0.0",
    "danger-plugin-digitalcitizenship": "*",
    "italia-tslint-rules": "*",
    "italia-utils": "^4.1.0",
    "jest": "^24.8.0",
    "jest-mock-express": "^0.1.1",
    "js-yaml": "^3.14.0",
    "modclean": "^3.0.0-beta.1",
    "npm-run-all": "^4.1.5",
    "oval": "^1.0.0",
    "prettier": "^1.18.2",
    "release-it": "^12.4.3",
    "shx": "^0.3.2",
    "ts-jest": "^24.0.2",
    "tslint": "^5.17.0",
    "typescript": "^3.9.0"
  },
  "dependencies": {
    "@types/node-fetch": "^2.5.7",
    "documentdb": "^1.15.3",
    "durable-functions": "^1.4.1",
    "express": "^4.15.3",
    "fp-ts": "1.17.4",
    "io-functions-commons": "^10.2.3",
    "io-functions-express": "^0.1.0",
    "io-ts": "1.8.5",
    "italia-ts-commons": "^8.1.0",
    "node-fetch": "^2.6.0",
    "soap": "^0.31.0",
    "winston": "^3.2.1"
  },
  "resolutions": {
    "fp-ts": "1.17.4",
    "io-ts": "1.8.5"
  }
}
