{
  "version": "2.0",
  "logging": {
    "logLevel": {
      "default": "Trace"
    }
  },
  "extensions": {
      "http": {
        "routePrefix": "/bonus"
      },

    "durableTask": {
      "hubName": "IoFunctionsBonus",
      "storageProvider": {
        "connectionStringName": "AzureWebJobsStorage"
      },
      "tracing": {
        "traceInputsAndOutputs": false,
        "traceReplayEvents": true
      },
      "useGracefulShutdown": true
    }
  }
}
